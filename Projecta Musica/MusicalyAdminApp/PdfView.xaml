<Window x:Class="MusicalyAdminApp.PdfView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusicalyAdminApp"
        xmlns:PdfViewer="clr-namespace:Syncfusion.Windows.PdfViewer;assembly=Syncfusion.PdfViewer.WPF"
        mc:Ignorable="d"
        Title="PdfView" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <PdfViewer:PdfViewerControl x:Name="pdfViewer" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ZoomMode="FitPage"/>
    </Grid>
</Window>
