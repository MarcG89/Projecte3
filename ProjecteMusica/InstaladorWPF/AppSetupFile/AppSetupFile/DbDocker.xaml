<Window x:Class="AppSetupFile.DbDocker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppSetupFile"
        mc:Ignorable="d"
        Title="DbDocker" MinHeight="205" MinWidth="560" Height="235" Width="590">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Escull la versió de Docker a instalar:"  />
                <ComboBox x:Name="opcionsDocker" Height="30" Width="100" />
            </StackPanel>
            <Button x:Name="btAccept" Content="Acceptar" Height="30" Margin="232,95,240,64" />
        </StackPanel>
    </Grid>
</Window>
